{
  "name": "wechaty-vorpal",
  "version": "0.2.11",
  "description": "Extensible Interactive CLI for Wechaty ChatOps, Powered by Vorpal.",
  "main": "dist/src/mod.js",
  "typings": "dist/src/mod.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "example": "ts-node examples/ding-dong-bot.ts",
    "start": "npm run example",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "lint:es": "eslint --ignore-pattern tests/fixtures/ '{bin,examples,scripts,src,tests}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/wechaty-vorpal.git"
  },
  "keywords": [
    "Wechaty",
    "CLI",
    "ChatOps",
    "Vorpal"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/wechaty-vorpal/issues"
  },
  "homepage": "https://github.com/wechaty/wechaty-vorpal#readme",
  "devDependencies": {
    "@chatie/eslint-config": "^0.8.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.10.1",
    "@types/cuid": "^1.3.1",
    "@types/vorpal": "^1.12.0",
    "pkg-jq": "^0.2.4",
    "shx": "^0.3.2",
    "tstest": "^0.4.10",
    "vorpal-hacker-news": "^1.0.6",
    "wechaty": "^0.41.15",
    "wechaty-puppet-mock": "^0.22.31"
  },
  "dependencies": {
    "cuid": "^2.1.8",
    "rxjs": "^6.5.5",
    "strip-ansi": "^6.0.0",
    "vorpal": "^1.12.0",
    "wechaty-plugin-contrib": "^0.11.4"
  },
  "peerDependencies": {
    "wechaty": "*"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  },
  "publishConfig": {
    "tag": "next"
  }
}
